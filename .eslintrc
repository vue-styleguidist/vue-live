{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/essential", "eslint:recommended"],
  "rules": {},
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "overrides": [
    {
      "files": ["**/__tests__/*.js"],
      "parserOptions": {
        "parser": "babel-eslint",
        "sourceType": "module"
      },
      "env": {
        "jest": true
      },
      "globals": {
        "mount": false,
        "shallowMount": false,
        "createComponentMocks": false
      }
    }
  ]
}
