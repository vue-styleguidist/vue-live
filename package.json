{
  "name": "vue-live",
  "version": "0.0.0-dev",
  "author": "Bart Ledoux <ledouxb@me.com>",
  "scripts": {
    "serve": "vite serve",
    "types": "vue-tsc --project ./tsconfig.app.json",
    "build:lib": "vite build --config ./vite.config.lib.ts",
    "types:lib": "vue-tsc --declaration --emitDeclarationOnly --project ./tsconfig.build.lib.json",
    "build:demo": "vite build",
    "start": "vite serve",
    "preview": "vite preview --port 4173",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "test:unit": "vitest run",
    "cy": "cypress open --e2e"
  },
  "dependencies": {
    "@vue/compiler-core": "^3.2.47",
    "@vue/compiler-sfc": "^3.2.47",
    "acorn": "^8.8.2",
    "acorn-walk": "^8.2.0",
    "core-js": "^3.30.1",
    "debounce": "^1.2.1",
    "hash-sum": "^2.0.0",
    "prismjs": "^1.29.0",
    "vue-inbrowser-compiler-sucrase": "^4.72.0",
    "vue-inbrowser-prismjs-highlighter": "^4.69.1",
    "vue-prism-editor": "^2.0.0-alpha.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.21.5",
    "@types/debounce": "^1.2.1",
    "@types/hash-sum": "^1.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.0.0",
    "@types/prismjs": "^1.26.0",
    "@uivjs/vue-github-corners": "^1.0.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/tsconfig": "^0.3.2",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.11.0",
    "prettier": "2.8.8",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4",
    "validate-commit-msg": "^1.1.3",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue": "^3.2.47",
    "vue-github-corners": "^1.2.3",
    "vue-tsc": "1.5.2",
    "vue3-datepicker": "^0.3.4"
  },
  "peerDependencies": {
    "vue": ">= 3.0.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11"
  ],
  "bugs": {
    "url": "https://github.com/vue-styleguidist/vue-live/issues"
  },
  "files": [
    "lib"
  ],
  "license": "MIT",
  "main": "lib/vue-live.umd.js",
  "module": "lib/vue-live.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/vue-live.mjs",
      "require": "./lib/vue-live.umd.js",
      "types": "./lib/index.d.ts"
    },
    "./style.css": "./lib/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-styleguidist/vue-live.git"
  }
}