{
  "name": "vue-live",
  "version": "0.0.0-dev",
  "author": "Bart Ledoux <ledouxb@me.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rollup -c",
    "lint": "vue-cli-service lint",
    "build:demo": "vue-cli-service build",
    "start": "vue-cli-service serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "debounce": "^1.2.0",
    "prismjs": "^1.16.0",
    "vue": "^2.6.10",
    "vue-inbrowser-compiler": "^3.13.10",
    "vue-prism-editor": "^0.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "raw-loader": "^2.0.0",
    "rollup": "^1.12.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "validate-commit-msg": "^2.14.0",
    "vuejs-datepicker": "^1.5.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/*.unit.js"
        ],
        "parserOptions": {
          "parser": "babel-eslint",
          "sourceType": "module"
        },
        "env": {
          "jest": true
        },
        "globals": {
          "mount": false,
          "shallowMount": false,
          "createComponentMocks": false
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/vue-styleguidist/vue-live/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/vue-live.common.js",
  "module": "dist/vue-live.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-styleguidist/vue-live.git"
  }
}
